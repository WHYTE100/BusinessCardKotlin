                android:layout_marginStart="@dimen/_24sdp"
                android:layout_marginBottom="@dimen/_5sdp"
                android:textSize="@dimen/_20ssp"
                android:textColor="?attr/colorOnPrimary"
                android:text="@string/social_media" />

